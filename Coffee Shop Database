INSERT into Coffee VALUES (10, "Iced Carmel", 4.00, 10, 8);
INSERT into Coffee VALUES (11, "Iced Tea", 5, 15, 6);
INSERT into Coffee VALUES (12, "Lemonade", 5, 20, 7);
INSERT into Coffee VALUES (13, "Water", 2, 20, 4);
INSERT into Coffee VALUES (14, "Sweet Tea", 3, 15, 5);
INSERT into Coffee VALUES (15, "Matacha", 4.50, 10, 6);

SELECT * FROM Coffee ORDER BY price;
SELECT menu, SUM(price) FROM Coffee GROUP BY id;
